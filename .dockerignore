# Ignore node_modules - will be installed inside the container
node_modules

# Ignore log files
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore .env files - manage environment variables securely outside the container
.env
.env.local
.env.*.local

# Ignore build output - generated by Next.js
.next
out

# Ignore Docker-related files
Dockerfile
docker-compose.yml
.dockerignore

# Ignore version control files and directories
.git
.gitignore

# Ignore editor and IDE specific files
.vscode
.idea
*.swp

# Ignore operating system files
.DS_Store
Thumbs.db

# Ignore test and temporary files
__tests__
*.test.js
*.spec.js
*.test.ts
*.spec.ts
coverage
*.tmp
*.temp
